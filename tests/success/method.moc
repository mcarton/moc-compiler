class Point {
    Int x;
    Int y;

    +(Point*) alloc {
        return new(Point);
    }

    -(Point*) init {
        x = 0;
        y = 0;
        return self;
    }

    -(Int) x {
        return 0;
    }

    -(Int) y {
        return 0;
    }

    -(void) x:(Int) a y:(Int) b {
        x = a;
        y = b;
    }
}

void main() {
    Point* point;
    [point x: 6 y: 7];
    Int a = [point x];
}

